{
  "Dhcp4": {
    "interfaces-config": {
      "interfaces": [
        "eth0",
        "eth1"
      ]
    },
    "control-socket": {
      "socket-type": "unix",
      "socket-name": "/run/kea/kea4-ctrl-socket"
    },
    "lease-database": {
      "type": "memfile",
      "lfc-interval": 3600
    },
    "valid-lifetime": 600,
    "max-valid-lifetime": 7200,
    "ddns-qualifying-suffix": "mic-a-linux.zz",
    "ddns-override-client-update": true,
    "subnet4": [
      {
        "id": 1,
        "subnet": "10.101.101.0/24",
        "pools": [
          {
            "pool": "10.101.101.10-10.101.101.100"
          }
        ],
        "option-data": [
          {
            "name": "routers",
            "data": "10.101.101.254"
          },
          {
            "name": "domain-name-servers",
            "data": "10.101.101.254"
          },
          {
            "name": "domain-name",
            "data": "mic-a-linux.zz"
          },
          {
            "name": "domain-search",
            "data": "mic-a-linux.zz"
          }
        ],
        "interface": "eth0"
      },
      {
        "id": 2,
        "subnet": "192.168.0.0/24",
        "pools": [
          {
            "pool": "192.168.0.10-192.168.0.100"
          }
        ],
        "option-data": [
          {
            "name": "routers",
            "data": "192.168.0.254"
          },
          {
            "name": "domain-name-servers",
            "data": "192.168.0.254"
          },
          {
            "name": "domain-name",
            "data": "mic-a-linux.zz"
          },
          {
            "name": "domain-search",
            "data": "mic-a-linux.zz"
          }
        ],
        "interface": "eth1"
      }
    ]
  }
}